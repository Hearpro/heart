{
  "name": "@cc-heart/object-to-declare",
  "version": "1.2.0",
  "description": "object2declare",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "debug": "node --loader ts-node/esm src/index.ts --inspect-brk=8888",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prettier": "npx prettier --write .",
    "prebuild": "rimraf dist types",
    "build": "npx rimraf dist && tsc -p tsconfig.build.json && rollup -c",
    "deploy": "npm run prebuild && npm run build"
  },
  "keywords": [
    "object2declare"
  ],
  "author": "carl chen",
  "license": "MIT",
  "exports": {
    ".": [
      {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.cjs",
        "types": "./dist/types/index.d.ts"
      }
    ]
  },
  "dependencies": {
    "@cc-heart/utils": "^4.0.4"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.4",
    "@types/node": "^18.18.1",
    "@vitest/coverage-v8": "^0.34.6",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "rollup": "^4.17.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^4.4.9",
    "vitest": "^0.34.6"
  }
}
